            game:GetService("Workspace").Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer, "Pastel blue-green")
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	    end
	end
    if Color then
		local colorcheck = ColorBox.Text
	    if colorcheck == ColorWord57 then
            saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            game:GetService("Workspace").Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer, "Pastel green")
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	    end
	end
    if Color then
		local colorcheck = ColorBox.Text
	    if colorcheck == ColorWord58 then
            saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            game:GetService("Workspace").Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer, "Pastel yellow")
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	    end
	end
    if Color then
		local colorcheck = ColorBox.Text
	    if colorcheck == ColorWord59 then
            saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            game:GetService("Workspace").Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer, "Pastel brown")
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	    end
	end
    if Color then
		local colorcheck = ColorBox.Text
	    if colorcheck == ColorWord60 then
            saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            game:GetService("Workspace").Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer, "Royal purple")
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	    end
	end
    if Color then
		local colorcheck = ColorBox.Text
	    if colorcheck == ColorWord61 then
            saved = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
            game:GetService("Workspace").Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer, "Hot pink")
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = saved
	    end
	end
end
ColorBox.FocusLost:Connect(MainTeamColors)


